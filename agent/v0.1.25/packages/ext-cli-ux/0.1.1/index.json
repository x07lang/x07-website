  "module_root": "modules",
  "meta": {
    "determinism_tier": "mixed",
    "worlds_allowed": [
      "solve-pure",
      "solve-fs",
      "solve-full",
      "run-os",
      "run-os-sandboxed"
    ],
    "requires_packages": [
      "ext-data-model@0.1.8",
      "ext-json-rs@0.1.4"
    ],
