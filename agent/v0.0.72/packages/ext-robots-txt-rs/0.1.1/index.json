{
  "schema_version": "x07.website.package-index@0.2.0",
  "name": "ext-robots-txt-rs",
  "version": "0.1.1",
  "description": "robots.txt parsing helpers (allow/disallow evaluation).",
  "docs": "robots.txt parsing helpers.\n\nModules:\n- ext.robots\n- ext.robots.tests\n\nUsage:\n- Add dependency: x07 pkg add ext-robots-txt-rs@0.1.1\n- Import ext.robots.\n",
  "module_root": "modules",
  "meta": {
    "determinism_tier": "pure",
    "worlds_allowed": [
      "solve-pure",
      "solve-fs",
      "solve-rr",
      "solve-kv",
      "solve-full",
      "run-os",
      "run-os-sandboxed"
    ],
    "requires_packages": [
      "ext-url-rs@0.1.3"
    ],
    "import_mode": "handwritten"
  },
  "modules": [
    {
      "module_id": "ext.robots",
      "path": "ext/robots.x07.json",
      "exports": [
        "ext.robots.allowed_v1"
      ]
    },
    {
      "module_id": "ext.robots.tests",
      "path": "ext/robots/tests.x07.json",
      "exports": [
        "ext.robots.tests.test_allowed_basic"
      ]
    }
  ]
}
