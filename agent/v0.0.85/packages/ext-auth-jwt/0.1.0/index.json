{
  "schema_version": "x07.website.package-index@0.2.0",
  "name": "ext-auth-jwt",
  "version": "0.1.0",
  "description": "Ed25519 + JWT (profiles pinned under arch/crypto). Deterministic token generation via canonical JSON payloads.",
  "docs": "Ed25519 + JWT helpers pinned by arch/crypto contracts.\n\nModules:\n- std.crypto.ed25519\n- std.auth.jwt\n- std.auth.jwt.tests\n\nWorlds:\n- run-os / run-os-sandboxed only\n\nUsage:\n- Add dependency: x07 pkg add ext-auth-jwt@0.1.0\n- Use std.auth.jwt.sign_from_arch_v1 / verify_from_arch_v1 with profiles under arch/crypto.\n",
  "module_root": "modules",
  "meta": {
    "determinism_tier": "os-world-only",
    "worlds_allowed": [
      "run-os",
      "run-os-sandboxed"
    ],
    "requires_packages": [
      "ext-base64-rs@0.1.4",
      "ext-data-model@0.1.7",
      "ext-json-rs@0.1.4",
      "ext-openssl-c@0.1.6"
    ],
    "import_mode": "handwritten",
    "visibility": "canonical"
  },
  "modules": [
    {
      "module_id": "std.auth.jwt",
      "path": "std/auth/jwt.x07.json",
      "exports": [
        "std.auth.jwt.sign_from_arch_v1",
        "std.auth.jwt.verify_from_arch_v1"
      ]
    },
    {
      "module_id": "std.auth.jwt.tests",
      "path": "std/auth/jwt/tests.x07.json",
      "exports": [
        "std.auth.jwt.tests.test_ed25519_rfc8032_vectors_v1",
        "std.auth.jwt.tests.test_jwt_profile_roundtrip_v1"
      ]
    },
    {
      "module_id": "std.crypto.ed25519",
      "path": "std/crypto/ed25519.x07.json",
      "exports": [
        "std.crypto.ed25519.sign_seed_v1",
        "std.crypto.ed25519.verify_seed_v1"
      ]
    }
  ]
}
