        "std.msg.rr.parse_v1",
        "std.msg.rr.record_deliver_v1",
        "std.msg.rr.record_publish_v1"
      ]
    },
    {
      "module_id": "std.msg.spec",
      "path": "std/msg/spec.x07.json",
      "exports": [
        "std.msg.spec.rr_policy_id_from_arch_v1",
        "std.msg.spec.topic_is_known_from_arch_v1",
