      ]
    },
    {
      "module_id": "std.msg.tests",
      "path": "std/msg/tests.x07.json",
      "exports": [
        "std.msg.tests.test_envelope_encode_decode_v1",
        "std.msg.tests.test_rr_entry_stability_v1"
      ]
    }
  ]
