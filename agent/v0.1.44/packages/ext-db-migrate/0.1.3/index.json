      "module_id": "std.db.migrate",
      "path": "std/db/migrate.x07.json",
      "exports": [
        "std.db.migrate.apply_from_arch_v1",
        "std.db.migrate.plan_parse_v1",
        "std.db.migrate.verify_from_arch_v1"
      ]
    },
    {
      "module_id": "std.db.migrate.tests",
