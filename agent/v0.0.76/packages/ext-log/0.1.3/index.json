{
  "schema_version": "x07.website.package-index@0.2.0",
  "name": "ext-log",
  "version": "0.1.3",
  "description": "Log record encoding and formatting helpers.",
  "docs": "Log record encoding and formatting helpers.\n\nModules:\n- ext.log\n- ext.log.tests\n\nUsage:\n- Add dependency: x07 pkg add ext-log@0.1.3\n- Import the modules listed above.\n",
  "module_root": "modules",
  "meta": {
    "determinism_tier": "pure",
    "worlds_allowed": [
      "solve-pure",
      "solve-fs",
      "solve-rr",
      "solve-kv",
      "solve-full",
      "run-os",
      "run-os-sandboxed"
    ],
    "import_mode": "handwritten"
  },
  "modules": [
    {
      "module_id": "ext.log",
      "path": "ext/log.x07.json",
      "exports": [
        "ext.log.format_record",
        "ext.log.is_valid_record",
        "ext.log.level_debug",
        "ext.log.level_error",
        "ext.log.level_info",
        "ext.log.level_name",
        "ext.log.level_trace",
        "ext.log.level_warn",
        "ext.log.record",
        "ext.log.record_level",
        "ext.log.record_msg",
        "ext.log.record_target"
      ]
    },
    {
      "module_id": "ext.log.tests",
      "path": "ext/log/tests.x07.json",
      "exports": [
        "ext.log.tests.test_format_record_empty_target",
        "ext.log.tests.test_invalid_record_formats",
        "ext.log.tests.test_record_roundtrip"
      ]
    }
  ]
}
